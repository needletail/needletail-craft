{
    "name": "needletail/needletail-craft",
    "description": "Needletail Search and Index package for Craft CMS",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "needletail"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Needletail",
            "homepage": "https://needletail.io"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0",
        "needletail/needletail-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "needletail\\needletail\\": "src/"
        }
    },
    "extra": {
        "name": "Needletail",
        "handle": "needletail",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/needletail-io/needletail-craft3/master/CHANGELOG.md",
        "class": "needletail\\needletail\\Needletail"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
